        "ridge = Ridge(alpha=1)\n",
        "lasso = Lasso(alpha=0.1, max_iter=50000, tol=0.01)\n",
        "ridge.fit(X_train_scaled, y_train)\n",
        "lasso.fit(X_train_scaled, y_train)\n",
        "\n",
        "plt.figure(figsize=(10,6))\n",
        "plt.plot(ridge.coef_, 'bo', label='Ridge Coefficients')\n",
        "plt.plot(lasso.coef_, 'ro', label='Lasso Coefficients')\n",
        "plt.xlabel('Feature Index')\n",
        "plt.ylabel('Coefficient Value')\n",
        "plt.title('Ridge vs Lasso Coefficient Magnitudes (Automobile Data)')\n",
